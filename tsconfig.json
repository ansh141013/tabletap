{
    "compilerOptions": {
        "target": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "frontend/src/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "frontend/node_modules"
    ]
}